{# Déclaraton des variables #}
{% set seo_title = "Resume - Rostand MIGAN" %}
{% set meta_description = "Découvrez le CV de Rostand MIGAN, Développeur Web Full Stack Freelance. Explorez mes
compétences, mon expérience professionnelle et mes réalisations en développement web et fintech." %}
{% set seo_image = absolute_url(asset('img/og_image/resume.png')) %}
{# End Déclaraton des variables #}
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{seo_title}}</title>
    <meta name="author" content="Rostand MIGAN">
    <meta name="description" content="{{meta_description}}">
    <meta property="og:site_name" content="Rostand MIGAN">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@100..900&family=Sora:wght@100..800&display=swap"
        rel="stylesheet">
    <link rel="icon"
        href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text><text y=%221.3em%22 x=%220.2em%22 font-size=%2276%22 fill=%22%23fff%22></text></svg>">

    <!-- Meta Keywords -->
    <meta name="keywords"
        content="Développeur Web, Développeur Web Full Stack, Freelance, Conception d'Applications Web, Création de Sites Web, Développement Front-End, Développement Back-End, React, Next.js, Symfony, Django, Node.js, JavaScript, TypeScript, PHP, Python, HTML, CSS, Tailwind CSS, WordPress, ERP, Fintech, Automatisation des Tâches, Scripts Python, Gestion de Base de Données, UX Design, Responsive Design, Sécurité Web, Performance Web, Scalabilité, Solutions Clé en Main, Start-Up, Entreprises, Particuliers, France, Canada, Bénin, Développement Web Personnalisé, Expertise SEO">

    <!-- Open Graph Metadata -->
    <meta property="og:title" content="{{seo_title}}">
    <meta property="og:description" content="{{meta_description}}">
    <meta property="og:image" content="{{seo_image}}">
    <meta property="og:url" content="{{ url(app.request.get('_route'), app.request.get('_route_params')) }}">
    <meta property="og:type" content="website">

    <!-- Twitter Card Metadata -->
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:title" content="{{seo_title}}">
    <meta name="twitter:description" content="{{meta_description}}">
    <meta name="twitter:image" content="{{seo_image}}">
    <meta name="twitter:creator" content="Rostand MIGAN">
    <meta property="twitter:url" content="{{ url(app.request.get('_route'), app.request.get('_route_params')) }}">

    <!-- Canonical Link -->
    <link rel="canonical" href="{{ url(app.request.get('_route'), app.request.get('_route_params')) }}">
    {% block javascripts %}
    {% block importmap %}{{ importmap('app') }}{% endblock %}
    {% endblock %}
</head>

<body class="xl:bg-white">
    <div class="w-full overflow-x-hidden">
        <main id="cv"
            class="min-h-dvh w-dvw xl:w-10/12 mx-auto my-10 lg:my-20 p-5 lg:p-10 bg-bgColor text-bodyColor/80">
            <header class="flex items-center justify-end gap-2">
                <span onclick="print()"
                    class="downloadCV cursor-pointer flex items-center justify-center gap-1 text-sm">
                    <span class="svg">
                        <svg>
                            <use href="#download" />
                        </svg>
                    </span>
                    <span>Download</span>
                </span>
                <div class="social-links flex justify-center items-center">
                    <a href="{{my_github}}" target="_blank" class="group relative inline-flex items-center svg text-sm">
                        <svg>
                            <use href="#github" />
                        </svg>
                        <span class="social-bubble">Github</span>
                    </a>
                </div>
            </header>

            <div class="cv-content flex flex-col-reverse md:flex-row-reverse items-start justify-end mt-6">
                <article class="w-full mt-3 md:mt-0 md:w-auto md:max-w-[70%] lg:max-w-[75%]">
                    <header class="cv_profil w-full border-b border-rostBlue/10 text-center md:text-left">
                        <h1 class="text-4xl md:text-7xl text-rostBlue font-bold mb-3 text-wrap">
                            Rostand <br class="hidden md:block">MIGAN
                        </h1>
                        <h2 class="text-xl md:text-2xl">Full Stack Developer and Fintech Enthusiast</h2>
                        <p class="font-normal mt-5 md:text-lg md:mt-10 mb-7 md:w-4/5 leading-normal md:leading-loose">
                            Full Stack Developer and Fintech Enthusiast dedicated to building dynamic, high-performance
                            web applications that accelerate digital transformation and financial innovation.
                        </p>
                    </header>

                    <section class="py-8">
                        <h3 class="external-link active w-fit text-2xl">
                            <span class="font-bold">Experience</span>
                        </h3>
                        <p class="md:w-4/5 text-lg mt-5">
                            Over the years, I've developed and managed several web projects, both for external
                            organizations and for my own companies.
                        </p>

                        <ul class="companies">
                            <li class="company">
                                <div class="header">
                                    <h3 class="text-rostBlue" translate="no">
                                        Compta Open
                                        <span class="text-bodyColor/75 font-normal relative before:content-['—']"
                                            translate="yes">
                                            &nbsp;Lead Developer & Founder
                                        </span>
                                    </h3>
                                    <span class="font-light text-base">March 2024 - Present</span>
                                </div>
                                <p class="description">
                                    Reinventing business management - I developed and led a pioneering online platform
                                    that facilitates knowledge sharing
                                    and effective management practices for business professionals. It has enabled
                                    real-time collaboration and integration of
                                    tools for an ever-growing user base specializing in accounting, tax and finance.
                                    <br>
                                    <small class="italic">Operational Focus: Benin, with a vision to expand
                                        globally.</small>
                                </p>
                                <ul class="tasks">
                                    <li>
                                        Built and led a cross-functional team of developers, designers, and content
                                        managers to deliver project milestones on
                                        time and within budget
                                    </li>
                                    <li>
                                        Built an article posting system comparable in power to Medium using
                                        React - Next.js
                                    </li>
                                    <li>
                                        Developed a robust Restful API infrastructure using Node.js - AdonisJs to
                                        support the whole app
                                    </li>
                                    <li>
                                        Collaborate with UX designers and Management Experts to optimize the management
                                        tools we make available to users
                                    </li>
                                    <li>
                                        Lead and participate in the testing of all application functionalities
                                    </li>
                                    <li>
                                        Optimized application performance by implementing delayed loading and caching
                                        techniques, ensuring fast loading times
                                        and smooth navigation.
                                    </li>
                                    <li>
                                        Foster an open and transparent development environment by making the project
                                        open source, encouraging collaboration and
                                        contribution from the developer community.
                                    </li>
                                    <li>
                                        Keep the project on track
                                    </li>
                                </ul>
                            </li>

                            <li class="company">
                                <div class="header">
                                    <h3 class="text-rostBlue" translate="no">
                                        Dommsoft
                                        <span class="text-bodyColor/75 font-normal relative before:content-['—']"
                                            translate="yes">
                                            &nbsp; Full Stack Developer
                                        </span>
                                    </h3>
                                    <span class="font-light text-base">May 2022 - March 2024</span>
                                </div>
                                <p class="description">
                                    Developed and maintained full-stack web applications, enhancing functionality and
                                    user experience through the
                                    integration of cutting-edge technologies. Collaborated closely with
                                    cross-disciplinary teams to deliver high-quality
                                    software solutions, meeting both technical specifications and business goals.
                                </p>
                                <ul class="tasks">
                                    <li>
                                        Developed robust APIs to facilitate seamless integrations and enhance
                                        application functionalities
                                    </li>
                                    <li>
                                        Crafted custom WordPress sites tailored to client specifications, focusing on
                                        optimal user engagement and performance
                                    </li>
                                    <li>
                                        Engineered dynamic web applications using Symfony and React, ensuring scalable,
                                        maintainable, and efficient solutions
                                    </li>
                                    <li>
                                        Implemented responsive designs and mobile-first strategies to ensure optimal
                                        viewing across various devices
                                    </li>
                                </ul>
                            </li>

                            <li class="company">
                                <div class="header">
                                    <h3 class="text-rostBlue" translate="no">
                                        Rost Digital Services
                                        <span class="text-bodyColor/75 font-normal relative before:content-['—']"
                                            translate="yes">
                                            &nbsp; Lead Developer
                                        </span>
                                    </h3>
                                    <span class="font-light text-base">January 2022 - Present</span>
                                </div>
                                <p class="description">
                                    I founded Rost Digital Services to manage my freelance activities, specializing in
                                    the digital transformation of businesses. As lead developer, I have led the creation
                                    of websites and applications that enhance digital integration for companies.
                                    Additionally, I have collaborated with various enterprises such as
                                    <span class="italic">Le Marché de La Com'</span> who outsource their project needs,
                                    extending
                                    my impact across multiple sectors.
                                </p>
                            </li>
                        </ul>
                    </section>

                    <section class="py-8">
                        <h3 class="external-link active w-fit text-2xl">
                            <span class="font-bold">Projects</span>
                        </h3>
                        <p class="md:w-4/5 text-lg mt-5">
                            You can find a selection of my projects at
                            <a href="{{path('app_work')}}" class="external-link active">rostand-migan.com/work</a>, and
                            I am available to
                            provide further details during a <a href="{{path('app_main_contact')}}"
                                class="external-link">scheduled
                                demonstration call</a>.
                        </p>
                    </section>
                </article>

                <aside class="w-full md:max-w-fit flex flex-col items-start gap-6 md:mr-10 lg:mr-20">
                    <section class="w-full py-0 contact-info flex flex-col">
                        <ul translate="no"
                            class="flex flex-row flex-wrap gap-2 text-sm justify-center font-light md:flex-col md:gap-1 md:text-base">
                            <li>
                                <a href="{{path('app_main')}}" class="external-link active group">
                                    <span>rostand-migan.com</span>
                                    <span class="sub-border group-hover:opacity-95 group-hover:scale-100"></span>
                                </a>
                            </li>
                            <li translate="yes">
                                <span>Dev Freelance</span>
                            </li>
                            <li>
                                <a href="mailto:hello@rostand-migan.com" class="external-link active group">
                                    <span>hello@rostand-migan.com</span>
                                    <span class="sub-border group-hover:opacity-95 group-hover:scale-100"></span>
                                </a>
                            </li>
                        </ul>
                    </section>

                    <div
                        class="technologies w-full flex justify-between gap-4 leading-relaxed text-sm md:text-base md:leading-relaxed md:flex-col">
                        <section class="py-0 coreTechnologies md:mb-4">
                            <h3 class="text-base md:text-lg leading-loose text-rostBlue font-semibold">
                                Core Technologies :
                            </h3>
                            <div class="divide-rostBlue/10 space-y-1" translate="no">
                                <ul class="list-inside list-disc space-y-1">
                                    <li>JavaScript</li>
                                    <li>Node Js</li>
                                    <li>PHP</li>
                                    <li>Python</li>
                                    <li>Sql</li>
                                </ul>

                                <ul class="list-inside list-disc space-y-1">
                                    <li>React</li>
                                    <li>Next Js</li>
                                    <li>Adonis Js</li>
                                    <li>Symfony</li>
                                    <li>WordPress</li>
                                </ul>

                                <ul class="list-inside list-disc space-y-1">
                                    <li>TypeScript</li>
                                    <li>Cypress</li>
                                    <li>Tailwind CSS</li>
                                    <li>Sass</li>
                                </ul>
                            </div>
                        </section>

                        <section class="py-0 others">
                            <h3 class="text-base md:text-lg leading-loose text-rostBlue font-semibold">
                                Others :
                            </h3>
                            <div class="divide-rostBlue/10">
                                <ul class="list-inside list-disc space-y-1">
                                    <li>Responsive Web Design</li>
                                    <li>Accessibility auditing</li>
                                    <li>SEO Optimization</li>
                                    <li>Robust & RESTful APIs</li>
                                    <li>Web Application Security</li>
                                    <li>Performance and Scalability</li>
                                    <li>Software Testing</li>
                                </ul>
                            </div>
                        </section>
                    </div>
                </aside>
            </div>

            <footer class="mt-10 h-24 flex items-center justify-start w-full">
                <a class="font-bold text-2xl transition-all menu-item group" href="{{path('app_main')}}" id="logo">
                    <span class="shadow-2xl">
                        &#8249;/&#8250; rostand
                    </span>
                    <span class=" sub-border group-hover:opacity-95 group-hover:scale-100"></span>
                </a>
            </footer>
        </main>
    </div>
    {% include "_partials/_icons.html.twig" %}
</body>

</html>